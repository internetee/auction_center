        <div class="c-header__tools" data-controller='toggle' data-toggle-toggle-class='is-hidden'>
            <% if user_signed_in? %>
                <a class="c-toolbar__notice" href="#" data-action="toggle#toggle">
                    <span>
                        <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="none">
                            <path fill="#fff"
                                d="M8 15.333A1.334 1.334 0 0 0 9.333 14H6.667A1.333 1.333 0 0 0 8 15.333Zm-5.333-2.666h10.666a.666.666 0 0 0 .472-1.138l-1.139-1.138V6.667a4.67 4.67 0 0 0-4-4.614v-.72a.667.667 0 1 0-1.333 0v.72a4.67 4.67 0 0 0-4 4.614v3.724l-1.138 1.138a.667.667 0 0 0 .472 1.138Zm1.804-1.529a.667.667 0 0 0 .196-.471v-4a3.333 3.333 0 1 1 6.666 0v4c0 .176.07.346.195.471l.196.195H4.276l.195-.195Z" />
                        </svg>
                        <sup>(<%= current_user&.notifications&.unread&.count %>)</sup>
                    </span>
                </a>

                <div class="c-notices is-hidden js-notices" data-toggle-target='element'>
                    <div class="js-notices-list">

                        <% current_user&.notifications&.unread&.limit(5)&.each do |n| %>
                        <div class="c-notice">
                        <div class="c-notice__header">
                            <%= component 'common/notifications/icon', notification: n, readed: false %>
                        </div>
                        <div class="c-notice__main"><%= n.to_notification.message %></div>
                        <div class="c-notice__footer">
                            <div class="c-notification__header__date"><%= I18n.t('.notification_received', datetime: I18n.l(n.created_at)) %></div>
                        </div>
                        </div>
                        <% end %>

                    </div>
                    <div class="c-notices__footer u-text-center">
                        <%= link_to t('notifications.show_all'), notifications_path, class: 'o-link' %>
                    </div>
                </div>
            <% end %>
            

            <% if user_signed_in? %>
              <%= link_to destroy_user_session_path, class: "c-header__user", method: :delete, data: { 'turbo-method': 'delete' } do %>
                <%= render 'svg/person' %><%= t(:sign_out) %>
              <% end %>
            <% else %>
              <%= link_to new_user_session_path, class: "c-header__user" do %>
                <%= render 'svg/person' %><%= t(:sign_in) %>
              <% end %>
            <% end %>

            <div class="c-header__lang">
                <div class="c-dropdown">
                    <span class="c-dropdown__label" onclick="this.parentNode.classList.toggle('is-open')">
                        <span class="c-dropdown__label__lang">
                            <%= current_language %>
                        </span>
                        <%= render 'svg/down_arrow' %>
                    </span>

                    <ul class="c-dropdown__list s-dropdown-list">
                        <%= languages %>
                    </ul>

                </div>
            </div>
        </div>
    </header>
